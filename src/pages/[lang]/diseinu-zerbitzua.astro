---
import DiseinuZerbitzuaES from "../../components/DiseinuZerbitzuaES.astro";
import DiseinuZerbitzuaEU from "../../components/DiseinuZerbitzuaEU.astro";

export async function getStaticPaths() {
    return [{ params: { lang: "es" } }, { params: { lang: "eu" } }];
}

export const prerender = true;
const { lang } = Astro.params;
---

{lang === "es" ? <DiseinuZerbitzuaES /> : <DiseinuZerbitzuaEU />}
